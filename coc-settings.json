{
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "dart": {
    "command": "dart",
    "args": [
      "/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
      "--lsp"
    ],
    "filetypes": ["dart"],
    "disableDynamicRegister": true,
    "trace.server": "verbose"
  },
  "tsserver.enableJavascript": true,
  "eslint.filetypes": ["javascript", "javascript.jsx","typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
  "coc.preferences.formatOnSaveFiletypes": ["css","scss", "json","javascript","javascriptreact","typescript","typescriptreact","html","dart","graphql"]
}
